{
    "manifest_version": 3,
    "name": "URL Crawler",
    "version": "1.0",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
    }
    ],
    
    "action": {
        "default_popup": "popup.html",
        "default_icon": "./images/icon.png"
    },
    "icons": {
      "48": "./images/icon.png",
      "128": "./images/icon.png"
    }
  }